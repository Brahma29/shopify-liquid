{{ 'base.css' | asset_url | stylesheet_tag }}
<script src="{{ 'webflow.js' | asset_url }}" defer="defer"></script>

<div data-w-id="7163af3e-52c6-1dcd-f3b7-32d72ae462bf" class="section para-section bg-transparent mx-0 px-2 wf-section">
  <div class="div-block-29">
    <div
      data-is-ix2-target="1"
      class="lottie-animation"
      data-w-id="7eedd081-08e5-17a3-e106-9c7bcb3b8f8b"
      data-animation-type="lottie"
      data-src="documents/button.json"
      data-loop="0"
      data-direction="1"
      data-autoplay="0"
      data-renderer="svg"
      data-default-duration="4.004003840917587"
      data-duration="0"
    ></div>
    <div class="div-block-28">
      <p
        class="para"
      >
        {{ section.settings.para_text }}
      </p>
      <a
        href="{{section.settings.btn_url}}"
        class="btn btn-bg-coco w-button"
        >KNOW MORE</a
      >
    </div>
  </div>
</div>

{% schema %}
{
  "name": "home_page_para",
  "class": "para-section",
  "settings": [
    {
      "type": "text",
      "id": "para_text",
      "default": "This is text",
      "label": "Paragraph"
    },
    {
      "type": "url",
      "id": "btn_url",
      "label": "Button Url"
    }
  ],
  "presets": [
    {
      "category": "home_page_para",
      "name": "Home Page Para"
    }
  ]
}
{% endschema %}
